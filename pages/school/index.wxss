page {
	background-color:white;
}
/* SEARCH START */
.search {
	height:100rpx;
	padding-top:15rpx;
	padding-bottom:15rpx;
	padding-left:15rpx;
	padding-right:15rpx;
	background-color:#fed406;
}
.search .keyword {
	text-align:center;
	height:70rpx;
	line-height:70rpx;
	padding-left:15rpx;
	padding-right:15rpx;
	border-radius:35rpx;
	font-size:26rpx;
	background-color:#F5F5F5;
	color:#2b2b2b;

	background-image:url(data:image/png;base64,);
	background-repeat:no-repeat;
	backgorund-position:70rpx center;
	background-size:70rpx 70rpx;
}
/* SEARCH END */
/* PROVINCES START */
.provinces {
	float:left;
	height:100vh;
	width:40vw;
	background-color:#F8F8F8;
}
.provinces .active {
	background-color:white;
	color:red;
}
.provinces .province {
	height:100rpx;
	line-height:100rpx;
	padding-left:25rpx;
	border-bottom:1px solid #EEE;
}
/* PROVINCES END */

/* SCHOOLS START */
.schools {
	float:right;
	height:100vh;
	width:55vw;
	margin-left:5vw;
	backgorund-color:white;
}
.schools .school {
	min-height:100rpx;
	line-height:100rpx;
	padding-right:20rpx;
	background-color:white;
	border-bottom:1rpx solid #E7E7E7;
}
/* PROVINCES END */
/* ITEMS START */
.clear {
	height:100rpx;
	line-height:100rpx;
	text-align:right;
	padding-right:30rpx;
}
