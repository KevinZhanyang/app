/* rule */
.rule-shadow {
	background-color:rgba(0, 0, 0, 0.5);
	position:fixed;
	top:0rpx;
	bottom:0rpx;
	right:0rpx;
	left:0rpx;
	width:100%;
	height:100%;
}
._rule {
	position:fixed;
	top:0rpx;
	bottom:0rpx;
	right:0rpx;
	left:0rpx;
	display:flex;
	justify-content: center;
	align-items: center;
	width:100vw;
	height:100vh;
}
._rule .rule {
	position:relative;
	width:80vw;
	min-height:300rpx;
	border-radius:15rpx;
	overflow:hidden;
	background-color:white;
}
._rule .rule .title {
	text-align:center;
	height:100rpx;
	padding-left:30rpx;
	padding-right:30rpx;
	line-height:100rpx;
	font-size:32rpx;
	font-weight:bold;
	background-color:#fed406;
	color:#6f6f6f;
}
._rule .rule .content {
	margin-top:30rpx;
	margin-left:30rpx;
	margin-right:30rpx;
	margin-bottom:50rpx;
	color:#6f6f6f;
	font-size:30rpx;
	line-height:40rpx;
}
._rule .rule .content .content_ {
	padding-top:20rpx;
	padding-bottom:20rpx;
}
._rule .rule .content .content_ + .content_ {
	border-top:1px dashed #EEE;
}
._rule .rule .close {
	height:100rpx;
	border-top:1px solid #F7F4F7;
	line-height:100rpx;
	text-align:center;
	color:#6f6f6f;
}
