}
._packets .attr .rule {
	color:#ff5555;
}
.packets {
	width:calc(100% - 60rpx);
	padding-top:15rpx;
	padding-bottom:30rpx;
	margin-right:30rpx;
	margin-left:30rpx;
}
.packets .packet {
	display:flex;
	justify-content: space-between;
	margin-bottom:15rpx;
}
.packets .packet .wait {
	height:51rpx;
	width:70rpx;
	background-color:gray;
	background-repeat:no-repeat;
	background-size:100% 100%;
	background-image:url(data:image/png;base64,);
}
.packets .packet .open {
	/*
	height:80rpx;
	width:110rpx;
	*/
	height:51rpx;
	width:70rpx;
	background-repeat:no-repeat;
	background-size:100% 100%;
	background-image:url(data:image/png;base64,);
}
.packets .packet .opened {
	height:51rpx;
	width:70rpx;
	background-repeat:no-repeat;
	background-size:100% 100%;
	background-image:url(data:image/png;base64,);
}
.packets .packet image {
	width:50rpx;
	height:50rpx;
}
.packets ._bar {
	position:relative;
	height:17rpx;
	overflow:hidden;
	border-radius:8rpx;
	border:1rpx solid #f9f9f9;
	background-color:#EEE;
}
.packets ._bar .vertical-bars {
	position:absolute;
	top:0rpx;
	bottom:0rpx;
	right:0rpx;
	left:0rpx;
	display:flex;
	justify-content:space-between;
}
.packets ._bar .vertical-bars .vertical-bar {
	text-align:center;
	background-color:#fed406;
	height:100%;
	width:4rpx;
	margin-right:33rpx;
	margin-left:33rpx;
}
.packets ._bar .bar {
	height:15rpx;
	border-radius:8rpx;
	background-color:#ff5555;
}
.packets .people {
	display:flex;
	justify-content: space-between;
	font-size:26rpx;
	color:2b2b2b
}
.packets .people .number {
	width:110rpx;
	text-align:center;
}
._congratulate {
	position:fixed;
	display:flex;
	justify-content: center;
	align-items: center;
	top:0rpx;
	right:0rpx;
	left:0rpx;
	bottom:0rpx;
	width:100%;
	height:100%;
	text-align:center;
	background-color:rgba(0,0,0,0.8);
}
.congratulate_ {
	top:none;
	bottom:-2000rpx;
	transition-property: bottom;
	transition-duration: 1s;
	transition-timing-function: linear;
	transition-delay: 2s;
}
._congratulate .congratulate {
	position:relative;
	width:600rpx;
	height:800rpx;
	padding-top:80rpx;
	padding-right:30rpx;
	padding-left:30rpx;
	padding-bottom:100rpx;
	border-radius:15rpx;
	background-image:url(http://static.124115.com/static/program/img/article/redpacket_background.png);
	background-size:100% 100%;
}
._congratulate .congratulate .content {
	margin-top:90rpx;
	font-size:30rpx;
	color:#FFE5AF;
}
._congratulate .congratulate .open {
	position:absolute;
	bottom:200rpx;
	right:0rpx;
	left:0rpx;
	width:180rpx;
	height:180rpx;
	border-radius:90rpx;
	overflow:hidden;
	background-color:#EE3E3A;
	background-image:url(http://static.124115.com/static/program/img/article/redpacket_open.png);
	background-size:100% 100%;
}
._congratulate .congratulate .close {
	position:absolute;
	left:0rpx;
	top:0rpx;
	width:90rpx;
	height:90rpx;
	opacity:0.5;
	font-size:30rpx;
	font-weight:bold;
	line-height:90rpx;
	color:black;
}
._congratulate .congratulate .bling {
	width:380rpx;
	height:270rpx;
	border-radius:8rpx;
	background-color:red;
}
._congratulate .congratulate .more {
	background-color:#fed406;
	color:white;
	font-size:26rpx;
	margin-top:30rpx;
}
