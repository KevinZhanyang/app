.pop-box {
  padding-bottom: 69rpx;
  height: 358rpx;
  width: 707rpx;
  margin: 0 auto;
  background: #fff;
  border-radius: 10rpx 10rpx 0 0;
  display: flex;
  align-items: center;
  justify-content: space-around;
  font-size: 30px;
  font-family: SimHei;
  font-weight: 400;
  color: rgba(43, 43, 43, 1);
}

.left-btn, .right-btn {
  display: flex;
  flex-direction: column;
  font-size: 24rpx;
  color: #333;
  justify-content: center;
}

.left-btn image, .right-btn image {
  width: 90rpx;
  height: 90rpx;
  border-radius: 50%;
  margin-bottom: 20rpx;
}

.scroll-header {
  position: fixed; /* 固定位置 */
  top: 0;
}

