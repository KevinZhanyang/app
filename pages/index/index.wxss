/* search */

.location_img {
  width: 30rpx;
  height: 36rpx;
  margin-right: 13rpx;
  margin-left: 24rpx;
  margin-top:10rpx;

}

.location {
  display: flex;
  flex-direction: row;
  height: 60rpx;
  line-height: 60rpx;
  text-align: bottom;
  width: 261rpx;
  display: -webkit-box;
  -webkit-line-clamp: 1;
  -webkit-box-orient: vertical;
  overflow: hidden;
  word-wrap: break-word;
  text-overflow: ellipsis;
}

.rule {
  display: -webkit-box;
  -webkit-line-clamp: 1;
  -webkit-box-orient: vertical;
  overflow: hidden;
  word-wrap: break-word;
  text-overflow: ellipsis;
  font-size: 26rpx;
  margin-top: -61rpx;
  width: 220rpx;
  height: 60rpx;
  margin-left: 62rpx;
  background: transparent !important;
  /* border: 1px  transparent  !important; */
  text-align: left;
  line-height: 60rpx;
  border:none !important;
}
/*去除buttun边框关键样式*/
button[class="rule"]::after {
  border: 0;
} 

._search {
  display: flex;
  padding-top: 15rpx;
  padding-bottom: 15rpx;
  padding-left: 15rpx;
  padding-right: 15rpx;
  background-color: #fed406;
}

.search {
  display: flex;
  padding-left: 75rpx;
  padding-right: 75rpx;
  position: relative;
  border-radius: 30rpx;
  background-color: #f5f5f5;
  border: 1px solid #f5f5f5;
  width: 461rpx;
  margin-left: 40rpx;
}

.search .magnifying_glass {
  position: absolute;
  top: 0rpx;
  left: 0rpx;
  bottom: 0rpx;
  height: 60rpx;
  width: 60rpx;
}

.search .magnifying_glass image {
  height: 60rpx;
  width: 60rpx;
}

.search .navigator {
  height: 60rpx;
  padding-left: 60rpx;
  padding-right: 60rpx;
  color: #999;
  font-size: 28rpx;
  line-height: 60rpx;
  text-align: center;
}

/* category */

._categories {
}

.categories {
  display: flex;
  flex-wrap: wrap;
  flex-direction: column;
  justify-content: flex-start;
  white-space: nowrap;
  width: 750rpx;
  height: 338rpx;
  padding-top: 7rpx;
  padding-bottom: 7rpx;
}

.categories .category {
  display: inline-block;
  width: 164rpx;
  text-align: center;
  font-size: 24rpx;
  padding-top: 25rpx;
  padding-bottom: 25rpx;
  height: 162rpx;
}

.categories .category .icon {
  width: 60rpx;
  height: 84rpx;
  margin-right: 52rpx;
  margin-left: 52rpx;
  padding-bottom: 24rpx;
}

.categories .category .icon image {
  width: 60rpx;
  height: 60rpx;
}

.categories .category .name {
  font-size: 28rpx;
  line-height: 28rpx;
}

/* carousels */

.carousels {
  /* margin-bottom:15rpx; */
  height: 312rpx;
}

.carousels .carousel {
}

.carousels .carousel .carousel-img {
  width: 100%;
  height: 312rpx;
}

/* switches */

.switches {
  display: flex;
  position: relative;
  margin-top: 20rpx;
}

.switches .switch {
  height: 88rpx;
  line-height: 88rpx;
  font-size: 34rpx;
  width: 50%;
  text-align: center;
  color: #333;
  font-weight: bold;
}

.switches .switch-left {
  background-image: url(http://static.124115.com/static/program/img/index/switch_left.png);
  background-repeat: no-repeat;
  background-size: 100% 100%;
}

.switches .switch-right {
  background-image: url(http://static.124115.com/static/program/img/index/switch_right.png);
  background-repeat: no-repeat;
  background-size: 100% 100%;
}

.switches .switch-off {
  color: #999;
}

.switches .switch-on {
  background-image: none;
  color: #fed406;
}

